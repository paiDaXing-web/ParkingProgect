  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.list .li .time1 > view {
  background-color: #0A2463;
  width: 16rpx;
  height: 16rpx;
  border-radius: 16rpx;
  margin-right: 10rpx;
}
